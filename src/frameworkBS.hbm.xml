<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name= "org.Test.modelClass.frameworkBS" table="ms_batch_status">
        <id name="BTCH_NBR" type="string">
            <column name="BTCH_NBR" length="50" />
        </id>

        <property name="BTCH_NM" type="string" column ="BTCH_NM" length="10" not-null="false" />
        <property name="INVOK_ID" type="string" column ="INVOK_ID" length="50" not-null="false" />
        <property name="STRT_DTTM" type="timestamp" column ="STRT_DTTM" length="6" not-null="false" />
        <property name="END_DTTM" type="timestamp" column ="END_DTTM" length="6" not-null="false" />
        <property name="BTCH_STS_CD" type="string" column ="BTCH_STS_CD" length="1" not-null="false" />
        <property name="RUN_MDE_TXT" type="string" column ="RUN_MDE_TXT" length="10" not-null="false" />
        <property name="CREAT_BY_NM" type="string" column ="CREAT_BY_NM" length="30" not-null="false" />
        <property name="CREAT_DTTM" type="timestamp" column ="CREAT_DTTM" length="6" not-null="false" />
        <property name="UPDT_BY_NM" type="string" column ="UPDT_BY_NM" length="30" not-null="false" />
        <property name="UPDT_DTTM" type="string" column ="UPDT_DTTM" length="6" not-null="false" />

    </class>

</hibernate-mapping>